{
  "name": "my-profile",
  "version": "1.0.0",
  "description": "My Profile Portfolio",
  "scripts": {
    "dev": "cd \"my profile/frontend\" && npm run dev",
    "build": "cd \"my profile/frontend\" && npm run build",
    "install:frontend": "cd \"my profile/frontend\" && npm install",
    "install:backend": "cd \"my profile/backend\" && npm install",
    "install:all": "npm run install:frontend && npm run install:backend"
  },
  "dependencies": {
    "mongoose": "^8.0.0"
  },
  "engines": {
    "node": ">=18.x"
  }
}
