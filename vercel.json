{
  "version": 2,
  "buildCommand": "cd \"my profile/frontend\" && npm install && npm run build",
  "outputDirectory": "my profile/frontend/dist",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
